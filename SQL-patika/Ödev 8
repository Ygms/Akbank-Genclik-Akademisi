1) CREATE TABLE employee (id SERIAL, name VARCHAR(50), birthday DATE, email VARCHAR(100));

3)
UPDATE employee
SET name = 'Ali'
WHERE id = 5
RETURNING *;

UPDATE employee
SET birthday = '2023-12-01'
WHERE id = 20
RETURNING *;

UPDATE employee
SET email = NULL
WHERE id = 1
RETURNING *;

UPDATE employee
SET email = 'kersk@engadget.com'
WHERE id = 18
RETURNING *;

UPDATE employee
SET name = 'Kazan'
WHERE id = 45
RETURNING *;

4)

DELETE FROM employee WHERE name = 'Kazan';
DELETE FROM employee WHERE name = 'Ali';
DELETE FROM employee WHERE id = 3;
DELETE FROM employee WHERE email = 'kersk@engadget.com';
DELETE FROM employee WHERE id = 23;

